{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-12-11/server.schema.json",
  "name": "io.github.verbart/tradernet-mcp",
  "description": "MCP server for Tradernet/Freedom24 trading API â€” portfolio, orders, quotes, and more",
  "repository": {
    "url": "https://github.com/verbart/tradernet-mcp",
    "source": "github"
  },
  "version": "1.1.0",
  "packages": [
    {
      "registryType": "npm",
      "identifier": "tradernet-mcp",
      "version": "1.1.0",
      "transport": {
        "type": "stdio"
      },
      "environmentVariables": [
        {
          "description": "Tradernet API public key",
          "isRequired": true,
          "format": "string",
          "isSecret": true,
          "name": "TRADERNET_PUBLIC_KEY"
        },
        {
          "description": "Tradernet API private key",
          "isRequired": true,
          "format": "string",
          "isSecret": true,
          "name": "TRADERNET_PRIVATE_KEY"
        }
      ]
    }
  ]
}
